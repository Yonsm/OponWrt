
config global
	option anon_swap '0'
	option auto_mount '1'
	option delay_root '5'
	option check_fs '0'
	option anon_mount '1'
	option auto_swap '0'

config mount
	option enabled '1'
	option label 'EMMC_BOOT'
	option target '/boot'
	option fstype 'vfat'

config mount
	option enabled '1'
	option label 'EMMC_ROOTFS1'
	option target '/mnt/mmcblk2p2'
	option fstype 'ext4'

config mount
	option enabled '1'
	option label 'EMMC_ROOTFS2'
	option target '/mnt/mmcblk2p3'
	option fstype 'ext4'

config mount
	option enabled '1'
	option label 'EMMC_SHARED'
	option target '/opt'
	option fstype 'ext4'

